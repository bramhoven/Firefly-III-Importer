@model Luciferin.Core.Entities.Settings.BooleanSetting

@Html.HiddenFor(m => m.Key)
<input id="@Html.IdFor(m => m.Value)" name="@Html.NameFor(m => m.Value)" type="checkbox" data-toggle="switchbutton" @(Model.Value ?? false ? "checked" : "") value="true" />
<input type="hidden" name="@Html.NameFor(m => m.Value)" value="false" />