@model Luciferin.BusinessLayer.Settings.Models.BooleanSetting

@Html.HiddenFor(m => m.Key)
<input id="@Html.IdFor(m => m.Value)" name="@Html.NameFor(m => m.Value)" type="checkbox" data-toggle="switchbutton" @(Model.Value ? "checked" : "") value="true" />
<input type="hidden" name="@Html.NameFor(m => m.Value)" value="false" />